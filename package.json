{
  "name": "@beezeelinx/ngx-tree",
  "version": "14.0.0",
  "description": "A derived version of angular-tree-component without mobx. Much Better performance.",
  "scripts": {
    "build": "ng build ngx-tree",
    "build:demo": "ng build demo --base-href /ngx-tree/docs/demo/ -c production",
    "start": "ng serve -o demo",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p projects/ngx-tree/tsconfig.doc.json -n ngx-tree -d dist/docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "ng lint --type-check",
    "test": "ng test ngx-tree",
    "release": "standard-version -a",
    "deploy": "npm run build && npm publish --access=public ./dist/ngx-tree --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beezeelinx/ngx-tree.git"
  },
  "author": "BeeZeeLinx <contact@beezeelinx.com>",
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beezeelinx/ngx-tree/issues"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/animations": "^14.2.12",
    "@angular/cli": "^14.2.10",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@compodoc/compodoc": "^1.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^14.18.34",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "core-js": "^3.2.0",
    "element-closest": "^3.0.0",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lodash-es": "^4.17.10",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "rxjs": "~7.5.0",
    "standard-version": "^4.1.0",
    "typescript": "~4.7.2",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node tools/bump-version.js && git add ."
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "homepage": "https://github.com/beezeelinx/ngx-tree#readme",
  "main": "karma.conf.js"
}
